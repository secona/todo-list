{
  "name": "todo-list",
  "version": "1.0.0",
  "description": "Fullstack Todo List Application",
  "author": "secona (https://github.com/secona)",
  "license": "MIT",
  "private": true,
  "scripts": {
    "watch:build:client": "parcel watch client/index.html --dist-dir dist/client",
    "watch:type-check:client": "tsc -w -p client",
    "watch:start:server": "ts-node-dev --project server/tsconfig.json --respawn server/index.ts",
    "build:client": "tsc -p client && parcel build client/index.html --dist-dir dist/client",
    "build:server": "tsc -p server",
    "dev": "run-p -r watch:**",
    "build": "run-s build:*",
    "start": "node dist/index.js",
    "clean": "rimraf .parcel-cache dist",
    "test": "jest"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.6.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "dotenv": "^9.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.11.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.7",
    "morgan": "^1.10.0",
    "nodemailer": "^6.6.0",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.9.0",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.3.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mongoose": "^5.10.5",
    "@types/morgan": "^1.9.2",
    "@types/nodemailer": "^6.4.1",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.10",
    "cross-env": "^7.0.3",
    "jest": "^27.0.3",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-beta.2",
    "ts-jest": "^27.0.2",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  }
}
